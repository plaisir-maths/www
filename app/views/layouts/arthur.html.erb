<!DOCTYPE html>
<%= render '/refinery/html_tag' %>
  <%# site_bar = render('/refinery/site_bar', :head => true) -%>
  <%= render '/refinery/head' %>
  <body>
    <%#= site_bar -%>
    <%= render '/refinery/ie6check' if request.env['HTTP_USER_AGENT'] =~ /MSIE/ -%>
    <div id="content" class="container_12">
      <header>
        <%= render '/layouts/arthur/header' -%>
      </header>
      <div id="primary-content" class="container_12">
        
        <%= yield %>
        
      </div>
    </div>
    <div class="clear"></div>
    <footer >
      <%= render '/layouts/arthur/footer' -%>
    </footer>
  </body>
</html>
